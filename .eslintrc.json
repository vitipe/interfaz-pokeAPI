  
{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always",
              "jsx": "never",
              "ts": "never",
              "tsx": "never"
            }
         ],
        "func-names": 0,
        "no-param-reassign": 0
    }
}